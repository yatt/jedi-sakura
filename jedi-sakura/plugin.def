[Plugin]
Id=jp.yatt.cmpl.jedi
Name=jedi-sakura
Description=bridge for jedi, Python autocomplete library
Type=wsh
Version=0.1
Url=https://d.hatena.ne.jp/yatt/
Author=yatt

[Wsh]
;UseCache=0
UseCache=1

[Plug]
Complement=jedi-sakura.vbs
Complement.Label=jedi-sakura

[Option]
O[1].Section=Option
O[1].Key=PyEnv
O[1].Label=Pythonフォルダ
O[1].Type=Dir
O[1].Type=C:\Python27
O[2].Section=Option
O[2].Key=CmplTempPath
O[2].Label=一時ファイル保存先
O[2].Type=Str
O[2].Type=C:\jedi-sakura-cmpl.txt
O[3].Section=Option
O[3].Key=EditingTempPath
O[3].Label=編集中ファイル一時保存先
O[3].Type=Str
O[3].Type=C:\jedi-sakura-editing.txt
