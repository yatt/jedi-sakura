[Plugin]
Id=jp.yatt.app.sakuraedior.plugin.jedi-sakura
Name=jedi-sakura
Description=bridge for jedi, Python autocomplete library
Type=wsh
Version=0.4
Url=https://github.com/yatt/jedi-sakura
Author=yatt

[Wsh]
UseCache=0
;UseCache=1

[Plug]
Complement=jedi-sakura.vbs
Complement.Label=jedi-sakura

[Option]
O[1].Section=Option
O[1].Key=PyEnv
O[1].Label=Pythonフォルダ
O[1].Type=Dir
O[1].Default=C:\Python27

O[2].Section=Option
O[2].Key=CmplTempPath
O[2].Label=一時ファイル保存先
O[2].Type=Str
O[2].Default=C:\jedi-sakura-cmpl.txt

O[3].Section=Option
O[3].Key=EditingTempPath
O[3].Label=編集中ファイル一時保存先
O[3].Type=Str
O[3].Default=C:\jedi-sakura-editing.txt

O[4].Section=Option
O[4].Key=PyStdErrPath
O[4].Label=インタプリタエラー出力先
O[4].Type=Str
O[4].Default=C:\jedi-sakura-stderr.txt

O[5].Section=Option
O[5].Key=LogEnabled
O[5].Label=（デバッグ用）ログ出力有無
O[5].Type=Bool
O[5].Default=False

O[6].Section=Option
O[6].Key=LogFilePath
O[6].Label=（デバッグ用）ログ保存先
O[6].Type=Str
O[6].Default=C:\jedi-sakura-log.txt


